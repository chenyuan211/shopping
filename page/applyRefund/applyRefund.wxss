page {
  background-color: #F7F7F7;
}
.refundShopping {
  background-color: white;
  padding: 0 10px;
}
.refundShopping_title {
  height: 44px;
  line-height: 44px;
  font-size: 14px;
  font-weight: 600;
}
.refundShopping-content {
  display: flex;
  padding: 10px 0;
}
.refundShopping-image {
  width: 80px;
  height: 80px;
  border-radius: 4px;
}
.refundShopping-image image {
  width: 100%;
  height: 100%;
  border-radius: 4px;
}
.refundShopping-infor {
  padding-left: 10px;
  width: 60%;
  flex-shrink: 0;
}
.refundShopping-infor-name {
  font-size: 14px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  width: 100%;
  overflow: hidden;
}
.refundShopping-infor view:last-child {
  font-size: 11px;
  padding-top: 4px;
}
.refundShopping-price {
  margin-left: auto;
  font-weight: bold;
  font-size: 14px;
}
.refundInfor, .supplementInfor {
  background-color: white;
  padding: 0 10px;
  margin-top: 8px;
}
.supplementInfor-content {
  width: 100%;
  border-radius: 4px;
  background-color: rgba(250, 250, 250, 1);
  height: 140px;
  margin: 8px 0;
}
.van-cell::after {
  border: 0 !important;
}
.van-cell-group::after {
  border: 0 !important;
}
.van-field {
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  position: relative;
}
.van-cell {
  padding-right: 0 !important;
}
.van-field__label {
  width: 200px !important;
  text-indent: 10px;
}
.van-field image {
  width: 20px;
  height: 20px;
  position: absolute;
  left: 0;
  top: 11px;
  z-index: 999;
}