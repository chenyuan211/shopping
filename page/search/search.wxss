/* page/search/search.wxss */
page {
  width: 100%;
  height: 100%;
}
.search-content {
  width: 100%;
  height: 100%;
  position: relative;
}
.productList-head {
  height: 68px;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  z-index: 999;
  background-color: white;
}
.return {
  padding-left: 10px;
  position: relative;
  height: 42px;
  overflow: hidden;
  width: 50px;
}
.return image {
  width: 16px;
  height: 16px;
}
.productList-head-search {
  position: relative;
  /* margin-left: 20px; */
  height: 42px;
  overflow: hidden;
}
.search-hint {
  color: rgba(51, 51, 51, 0.6);
  font-size: 12px;
  height: 35px;
  line-height: 45px;
  text-align: center;
}
.search-input {
  width: 210px;
  height: 30px;
  background: #F7F7F7;
  border-radius: 14px;
  position: relative;
}
.search-input input {
  width: 80%;
  position: absolute;
  left: 32px;
  height: 30px;
  line-height: 30px;
  font-size: 12px;
}
.search-input image {
  width: 17px;
  height: 17px;
  position: absolute;
  top: 7px;
  left: 11px;
}
.search-input {
  display: flex;
  padding: 0 10px;
  margin-top: 10px;
}
.search-inputs {
  width: 90%;
  height: 32px;
  line-height: 32px;
  background: #F7F7F7;
  border-radius: 16px;
  border: 1px solid #02AAB0;
  position: relative;
}
.search-input input {
  width: 80%;
  position: absolute;
  left: 32px;
  height: 30px;
  line-height: 30px;
  font-size: 12px;
}
.search-input image {
  width: 17px;
  height: 17px;
  position: absolute;
  top: 7px;
  left: 11px;
}
.search-input text {
  margin-left: auto;
  color: rgba(51, 51, 51, 0.6);
  font-size: 14px;
  line-height: 32px;
}
.topSearch {
  padding: 0 10px;
  position: fixed;
  height: 100%;
  width: 100%;
  overflow: hidden;
  background-color: white;
}
.topSearch-title{
  font-size: 14px;
  color: rgba(51, 51, 51, 0.85);
  font-weight: 600;
}
.topSearch-list {
  display: flex;
  padding-top: 12px;
  flex-direction: row;
  flex-wrap: wrap;
}
.topSearch-list view {
  padding: 0 11px;
  height: 27px;
  line-height: 28px;
  color: rgba(51, 51, 51, 1);
  font-size: 12px;
  margin-right: 10px;
  margin-bottom: 15px;
  background: #F5F5F5;
  border-radius: 27px;
}
.search-result {
  padding-left: 20px;
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: white;
}
.search-result .search-result-lists{
  height: 44px;
  line-height: 44px;
  font-size: 12px;
  width: 100%;
}
.search-result-list {
  display: flex;

}
.search-result-list view {
  display: inline-block;
}